package edu.cpp.cs240.hash;

public class IntHashTableEntry {

	private int key;
	private int value;
	private IntHashTableEntry next;

	public IntHashTableEntry(int key, int value) {
		this.key = key;
		this.value = value;
	}

	public int getKey() {
		return key;
	}
	public void setKey(int key) {
		this.key = key;
	}
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}

	public IntHashTableEntry getNext() {
		return next;
	}

	public void setNext(IntHashTableEntry next) {
		this.next = next;
	}

}
