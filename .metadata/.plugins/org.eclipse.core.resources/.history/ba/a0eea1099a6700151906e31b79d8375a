package edu.cpp.cs240;

public class SLLTest {

	public static void main(String[] args) {
		SLinkedList sll = new SLinkedList();
		sll.add(3);
		System.out.println(sll);
		System.out.println(sll.size());

		sll.add(5);
		System.out.println(sll);
		System.out.println(sll.size());

		sll.add(7);

		sll.add(0, 1);
		sll.add(2, 4);
		sll.add(2, 55);

		System.out.println(sll);
		System.out.println(sll.size());

		sll.remove(0);
		System.out.println(sll);
		System.out.println(sll.size());

		sll.remove(2);
		System.out.println(sll);
		System.out.println(sll.size());

		sll.remove(2);
		System.out.println(sll);
		System.out.println(sll.size());


		sll.remove(2);
		System.out.println(sll);

	}

}
