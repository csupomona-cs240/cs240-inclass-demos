package edu.cpp.cs240;

public class IntArrayListTester {

	public static void main(String[] args) {
		IntArrayList myList = new IntArrayList();
		myList.add(5);
		myList.add(7);

		System.out.println(myList);
		System.out.println("Size: " + myList.size());

		myList.add(9);
		myList.add(11);
		myList.add(9);
		myList.add(11);
		myList.add(9);
		myList.add(11);
		myList.add(9);
		myList.add(11);
		myList.add(9);
		myList.add(11);
		myList.add(9);
		myList.add(11);
		System.out.println(myList);
		System.out.println("Size: " + myList.size());

		myList.add(11, 100);

		myList.set(2, 88);
		System.out.println(myList);
		System.out.println("Size: " + myList.size());
		System.out.println("1st element in the list: " + myList.get(0));

		System.out.println("First index of 5: " + myList.indexOf(5));
		System.out.println("First index of 5555: " + myList.indexOf(5555));

		myList.add(2, 55);
		System.out.println(myList);
		System.out.println("Size: " + myList.size());

		myList.remove(1);
		System.out.println(myList);
		System.out.println("Size: " + myList.size());

//		myList.clear();
//		System.out.println(myList);
//		System.out.println("Size: " + myList.size());
	}

}
